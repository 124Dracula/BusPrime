<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="backend.BD.Coordenadas" table="Coordenadas" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="coordX" column="CoordX" type="double" not-null="true" lazy="false"/>
		<property name="coordY" column="CoordY" type="double" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
